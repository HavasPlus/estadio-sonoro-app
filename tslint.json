{
  "defaultSeverity": "error",
  "extends": ["tslint:all", "tslint-config-airbnb", "tslint-config-prettier", "tslint-react-a11y", "tslint-react"],
  "jsRules": {},
  "rules": {
    "import-name": false,
    "ordered-imports": false,
    "no-angle-bracket-type-assertion": false,
    "no-reference": false,
    "no-implicit-dependencies": false,
    "no-empty": true,
    "no-submodule-imports": false,
    "no-object-literal-type-assertion": false,
    "no-empty-interface": false,
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "file-name-casing": [true, "kebab-case"],
    "object-shorthand-properties-first": false,
    "no-string-literal": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "no-null-keyword": false,
    "jsx-no-multiline-js": false,
    "completed-docs": false,
    "no-restricted-globals": false,
    "prefer-readonly": false,
    "promise-function-async": false,
    "await-promise": false,
    "esModuleInterop": true,
    "no-floating-promises": false,
    "prefer-method-signature": false,
    "no-inferrable-types": true,
    "typedef": false,
    "no-unused-expression": true,
    "no-unused-variable": true,
    "no-magic-numbers": true,
    "match-default-export-name": false,
    "quotemark": false,
    "missing-jsdoc": false,
    "no-default-import": false,
    "no-relative-imports": false,
    "export-name": false,
    "max-line-length": false,
    "no-require-imports": false,
    "react-a11y-role": false,
    "react-this-binding-issue": false,
    "no-unsafe-any": false,
    "align": false,
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
    "no-import-side-effect": [
      true,
      {
        "ignore-module": "(\\.html|\\.scss)$"
      }
    ],
    "max-func-body-length": 10,
    "strict-boolean-expressions": false,
    "no-any": false,
    "interface-over-type-literal": false,
    "no-default-export": false,
    "no-use-before-declare": false,
    "strict-type-predicates": false,
    "no-void-expression": false
  },
  "linterOptions": {
    "exclude": ["./node_modules"]
  },
  "rulesDirectory": ["tslint-misc-rules"]
}
